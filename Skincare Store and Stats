SELECT SUM (quantity) FROM skincare;
SELECT * FROM skincare 
WHERE "price" >= 100
ORDER BY price asc;

SELECT * FROM skincare 
WHERE "quantity" <=5
ORDER By quantity desc;

SELECT * FROM skincare 
ORDER BY aisle asc; 

SELECT id FROM skincare;

SELECT * from skincare 
ORDER by price 
